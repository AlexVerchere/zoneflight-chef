<VirtualHost *:80>
    ServerName www.zoneflight.ovh
    ServerAdmin webmaster@localhost

    DocumentRoot /var/www/zoneflight-web/public/
    <Directory "/var/www/zoneflight-web/public/">
        AllowOverride All
        Order allow,deny
        Allow from all
    </Directory>
    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
